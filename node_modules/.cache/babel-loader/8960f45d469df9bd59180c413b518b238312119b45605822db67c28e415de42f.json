tive-link\");\r\n            }\r\n            this.tabName = tabNameAssign;\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Galaxy Fold */\r\n    .about-wrapper{\r\n        width: 100%;\r\n        color: #4E6C50;\r\n        height: 100%;\r\n    }\r\n        .about-main-container{\r\n            padding-top: 30px;\r\n            width: 100%;\r\n        }\r\n            .container-one{\r\n                height: 100%;\r\n                width: 100%;\r\n            }   \r\n                .profile-section{\r\n\r\n                    padding-bottom: 10px;\r\n                }\r\n                    .profile-section img{\r\n                        height: 150px;\r\n                        width: 150px;\r\n                        border-radius: 15%;\r\n                        object-fit: cover;\r\n                        object-position: 100% 0;\r\n                    }\r\n            .container-two{\r\n                height: 100%;\r\n            }\r\n                .container-two h4{\r\n                    margin-top: 10px;\r\n                    margin-bottom: 10px;\r\n                    color: #395144;\r\n                }\r\n                .about-me,.about-buttons{\r\n                    text-align: left;\r\n                }\r\n                    .about-me p{\r\n                        text-indent: 40px;\r\n                        margin-top: 0;\r\n                        font-size: 14px;\r\n                        font-family: 'Roboto', sans-serif;\r\n                        text-align: justify;\r\n                    }\r\n                    .about-buttons{\r\n                        display: flex;\r\n                        padding-top: 20px;\r\n                    }\r\n\r\n                        .about-links{\r\n                            font-family: 'Poppins', sans-serif;\r\n                            color: #395144;\r\n                            margin-top: 5px;\r\n                            margin-right: 20px;\r\n                            position: relative;\r\n                            cursor: pointer;\r\n                        }\r\n                        .about-links::after{\r\n                            content: '';\r\n                            width: 0;\r\n                            height: 3px;\r\n                            background: #AA8B56;\r\n                            position: absolute;\r\n                            left: 0;\r\n                            bottom: -8px;\r\n                            transition: 0.5s;\r\n                        }\r\n                        .about-links.active-link::after{\r\n                            width: 60%;\r\n                        }\r\n                    .skills-tab{\r\n                        display: flex;\r\n                        flex-wrap: wrap;\r\n                        justify-content: center;\r\n                        padding-top: 35px;\r\n                        gap: 20px;\r\n                    }\r\n                        .skills-content{\r\n                            /* background-color: rgba(170, 139, 86, 0.34); */\r\n                            background-color: rgba(240, 235, 206,0.74);\r\n                            height: 100px;\r\n                            width: 100px;\r\n                            border-radius: 10px;\r\n                            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);\r\n                        }\r\n                            .icon-class{\r\n                                color: #395144;\r\n                                font-size: 36px;\r\n                                padding: 20px 0 0 0;\r\n                            }\r\n                            .skills-content p{\r\n                                margin: 10px 0;\r\n                                font-size: 12px;\r\n                            }\r\n                            \r\n                    .experiences-tab{\r\n                        padding-top: 35px;\r\n                    }\r\n                        .experiences-content{\r\n                            text-align: left;\r\n                            height: 90px;\r\n                            font-size: 10px;\r\n                            /* background-color: rgba(170, 139, 86, 0.34); */\r\n                            background-color: rgba(240, 235, 206,0.74);\r\n                            margin-top: 10px;\r\n                            padding: 13px 0 5px 15px;\r\n                            border-radius: 10px;\r\n                            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);\r\n                        }\r\n                            .experiences-content strong{\r\n                                color: #395144;\r\n                            }\r\n                            .experiences-content p{\r\n                                margin: 5px 0;\r\n                                font-size: 10px;\r\n                            }\r\n                    .education-tab{\r\n                        padding-top: 35px;\r\n                    }\r\n                        .education-content{\r\n                            text-align: left;\r\n                            height: 90px;\r\n                            font-size: 10px;\r\n                            /* background-color: rgba(170, 139, 86, 0.34); */\r\n                            background-color: rgba(240, 235, 206,0.74);\r\n                            margin-top: 5px;\r\n                            padding: 13px 5px 5px 15px;\r\n                            border-radius: 10px;\r\n                            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);\r\n                        }\r\n                            .education-content strong{\r\n                                color: #395144;\r\n                            }\r\n                            .education-content p{\r\n                                font-size: 10px;\r\n                            }        \r\n\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","exter