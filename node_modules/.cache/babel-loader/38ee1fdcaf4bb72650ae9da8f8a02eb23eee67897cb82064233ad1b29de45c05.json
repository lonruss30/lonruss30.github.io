n                        font-family: 'Roboto', sans-serif;\r\n                        text-align: justify;\r\n                    }\r\n                    .about-buttons{\r\n                        display: flex;\r\n                        padding-top: 20px;\r\n                    }\r\n\r\n                        .about-links{\r\n                            font-family: 'Poppins', sans-serif;\r\n                            color: #395144;\r\n                            margin-top: 5px;\r\n                            margin-right: 50px;\r\n                            position: relative;\r\n                            cursor: pointer;\r\n                        }\r\n                        .about-links::after{\r\n                            content: '';\r\n                            width: 0;\r\n                            height: 3px;\r\n                            background: #AA8B56;\r\n                            position: absolute;\r\n                            left: 0;\r\n                            bottom: -8px;\r\n                            transition: 0.5s;\r\n                        }\r\n                        .about-links.active-link::after{\r\n                            width: 60%;\r\n                        }\r\n                    .skills-tab{\r\n                        display: flex;\r\n                        justify-content: center;\r\n                        padding-top: 35px;\r\n                        gap: 20px;\r\n                    }\r\n                        .skills-content{\r\n                            background-color: rgba(170, 139, 86, 0.34);\r\n                            height: 80px;\r\n                            width: 80px;\r\n                            border-radius: 10px;\r\n                            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);\r\n                        }\r\n                            .skills-content p{\r\n                                margin: 10px 0;\r\n                                font-size: 12px;\r\n                                \r\n                            }\r\n                            .icon-class{\r\n                                font-size: 32px;\r\n                                padding: 10px 0 0 0;\r\n                            }\r\n\r\n@media only screen and (max-width: 1450px){\r\n    .container-one{\r\n        width: 40%;\r\n        height: 100%;\r\n        padding-right: 50px;\r\n    }\r\n        .profile-section img{\r\n            height: 450px;\r\n        }\r\n    .container-two{\r\n        height: 100%;\r\n        width: 50%;\r\n        padding-right: 50px;\r\n    }\r\n        .skills-tab{\r\n            flex-wrap: wrap;\r\n        }\r\n}\r\n\r\n/* Iphone XR */\r\n@media only screen and (max-width: 414){\r\n    .about-wrapper{\r\n        height: inherit;\r\n    }\r\n\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}