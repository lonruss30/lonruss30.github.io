-section img{\r\n                    height: 250px;\r\n                    border-radius: 5%;\r\n                }\r\n            .container-two{\r\n                height: 100%;\r\n            }\r\n                .container-two h1{\r\n                    margin-top: 10px;\r\n                    margin-bottom: 10px;\r\n                    color: #395144;\r\n                }\r\n                .about-me,.about-buttons{\r\n                    text-align: left;\r\n                }\r\n                    .about-me p{\r\n                        text-indent: 30px;\r\n                        margin-top: 0;\r\n                        font-family: 'Roboto', sans-serif;\r\n                        text-align: justify;\r\n                    }\r\n                    .about-buttons{\r\n                        display: flex;\r\n                        padding-top: 20px;\r\n                    }\r\n\r\n                        .about-links{\r\n                            font-family: 'Poppins', sans-serif;\r\n                            color: #395144;\r\n                            margin-top: 5px;\r\n                            margin-right: 20px;\r\n                            position: relative;\r\n                            cursor: pointer;\r\n                        }\r\n                        .about-links::after{\r\n                            content: '';\r\n                            width: 0;\r\n                            height: 3px;\r\n                            background: #AA8B56;\r\n                            position: absolute;\r\n                            left: 0;\r\n                            bottom: -8px;\r\n                            transition: 0.5s;\r\n                        }\r\n                        .about-links.active-link::after{\r\n                            width: 60%;\r\n                        }\r\n                    .skills-tab{\r\n                        display: flex;\r\n                        flex-wrap: wrap;\r\n                 