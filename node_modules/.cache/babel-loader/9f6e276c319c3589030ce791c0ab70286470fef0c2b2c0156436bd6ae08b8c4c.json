           <h5 class=\"about-links\" @click=\"openTab('experiences')\">Experiences</h5>\r\n                        <h5 class=\"about-links\" @click=\"openTab('education')\">Education</h5>\r\n                    </div>\r\n                    <div v-if=\"tabName == 'skills'\" class=\"skills-tab\">\r\n                        <div v-for=\"(items, index) in informationData.Technologies\" v-bind:key=\"items\"\r\n                            class=\"skills-content active-tab\">\r\n                            <i :class=\"[informationData.Technologies[index].Icon]\"></i>\r\n                            <p>{{ items.Title }}</p>\r\n                        </div>\r\n                    </div>\r\n                    <div v-if=\"tabName == 'experiences'\" class=\"experiences-tab\">\r\n                        <div v-for=\"(items) in informationData.ExperienceArray\" v-bind:key=\"items\"\r\n                            class=\"experiences-content\">\r\n                            <strong>{{ items.Company }}</strong>\r\n                            <p>{{ items.Date }}</p>\r\n                            <p><strong>Role:</strong> {{ items.Role }}</p>\r\n                        </div>\r\n                    </div>\r\n                    <div v-if=\"tabName == 'education'\" class=\"education-tab\">\r\n                        <div v-for=\"(items) in informationData.EducationArray\" v-bind:key=\"items\"\r\n                            class=\"education-content\">\r\n                            <strong>{{ items.School }}</strong>\r\n                            <p>{{ items.EducationalLevel }} {{ items.Date }}</p>\r\n                            <p>{{ items.Course }}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport infoData from \"../data/info.json\"\r\nvar tabLinks = document.getElementsByClassName(\"about-links\");\r\nexport default {\r\n    data() {\r\n        return {\r\n            tabName: 'skills',\r\n            informationData: infoData\r\n        }\r\n\r\n    },\r\n    methods: {\r\n\r\n        openTab(tabNameAssign) {\r\n            Array.prototype.forEach.call(tabLinks, (tablink) => {\r\n                tablink.classList.remove(\"active-link\");\r\n            });\r\n            event.currentTarget.classList.add(\"active-link\");\r\n            this.tabName = tabNameAssign;\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Galaxy Fold */\r\n.about-wrapper {\r\n    width: 100%;\r\n    color: #4E6C50;\r\n    height: 100%;\r\n}\r\n\r\n.about-main-container {\r\n    padding-top: 30px;\r\n    width: 100%;\r\n}\r\n\r\n.container-one {\r\n    height: 100%;\r\n    width: 100%;\r\n}\r\n\r\n.profile-section {\r\n    padding-bottom: 10px;\r\n}\r\n\r\n.profile-section img {\r\n    height: 150px;\r\n    width: 150px;\r\n}\r\n\r\n@media only screen and (min-width: 280px) {\r\n    .profile-section img {\r\n        border-radius: 15%;\r\n        object-fit: cover;\r\n        object-position: 100% 0;\r\n    }\r\n}\r\n\r\n.container-two {\r\n    height: 100%;\r\n}\r\n\r\n.container-two h4 {\r\n    margin-top: 10px;\r\n    margin-bottom: 10px;\r\n    color: #395144;\r\n}\r\n\r\n.about-me,\r\n.about-buttons {\r\n    text-align: left;\r\n}\r\n\r\n.about-me p {\r\n    text-indent: 40px;\r\n    margin-top: 0;\r\n    font-size: 14px;\r\n    font-family: 'Roboto', sans-serif;\r\n    text-align: justify;\r\n}\r\n\r\n.about-buttons {\r\n    display: flex;\r\n    padding-top: 20px;\r\n}\r\n\r\n.about-links {\r\n    font-family: 'Poppins', sans-serif;\r\n    color: #395144;\r\n    margin-top: 5px;\r\n    margin-right: 20px;\r\n    position: relative;\r\n    cursor: pointer;\r\n}\r\n\r\n.about-links::after {\r\n    content: '';\r\n    width: 0;\r\n    height: 3px;\r\n    background: #AA8B56;\r\n    position: absolute;\r\n    left: 0;\r\n    bottom: -8px;\r\n    transition: 0.5s;\r\n}\r\n\r\n.about-links.active-link::after {\r\n    width: 60%;\r\n}\r\n\r\n.skills-tab {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n    padding-top: 45px;\r\n    gap: 20px;\r\n}\r\n\r\n.skills-content {\r\n    /* background-color: rgba(170, 139, 86, 0.34); */\r\n    background-color: rgba(240, 235, 206, 0.74);\r\n    height: 100px;\r\n    width: 100px;\r\n    border-radius: 10px;\r\n    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);\r\n}\r\n\r\n.icon-class {\r\n    color: #395144;\r\n    font-size: 36px;\r\n    padding: 20px 0 0 0;\r\n}\r\n\r\n.skills-content p {\r\n    margin: 10px 0;\r\n    font-size: 12px;\r\n}\r\n\r\n.experiences-tab {\r\n    padding-top: 35px;\r\n}\r\n\r\n.experiences-content {\r\n    text-align: left;\r\n    height: 90px;\r\n    font-size: 10px;\r\n    /* background-color: rgba(170, 139, 86, 0.34); */\r\n    background-color: rgba(240, 235, 206, 0.74);\r\n    margin-top: 10px;\r\n    padding: 13px 0 5px 15px;\r\n    border-radius: 10px;\r\n    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);\r\n}\r\n\r\n.experiences-content strong {\r\n    color: #395144;\r\n}\r\n\r\n.experiences-content p {\r\n    margin: 5px 0;\r\n    font-size: 10px;\r\n}\r\n\r\n.education-tab {\r\n    padding-top: 35px;\r\n}\r\n\r\n.education-content {\r\n    text-align: left;\r\n    height: 90px;\r\n    font-size: 10px;\r\n    /* background-color: rgba(170, 139, 86, 0.34); */\r\n    background-color: rgba(240, 235, 206, 0.74);\r\n    margin-top: 10px;\r\n    padding: 13px 5px 5px 15px;\r\n    border-radius: 10px;\r\n    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.08);\r\n}\r\n\r\n.education-content strong {\r\n    color: #395144;\r\n}\r\n\r\n.education-content p {\r\n    font-size: 10px;\r\n}\r\n\r\n/* Iphone XR */\r\n@media only screen and (min-width: 410px) {\r\n\r\n    .container-two h4 {\r\n        font-size: large;\r\n    }\r\n\r\n    .about-me p {\r\n        font-size: 16px;\r\n    }\r\n\r\n    .about-links {\r\n        font-size: medium;\r\n    }\r\n\r\n    /* Skills Tab */\r\n    .skills-content {\r\n        height: 150px;\r\n        width: 150px;\r\n    }\r\n\r\n    .icon-class {\r\n        color: #395144;\r\n        font-size: 48px;\r\n        padding: 30px 0 0 0;\r\n    }\r\n\r\n    .skills-content p {\r\n        margin: 15px 0;\r\n        font-size: 16px;\r\n    }\r\n\r\n    /* Experiences Tab */\r\n    .experiences-content {\r\n        height: 110px;\r\n        font-size: 14px;\r\n        padding: 13px 0 5px 15px;\r\n    }\r\n\r\n    .experiences-content p {\r\n        margin: 5px 0;\r\n        font-size: 14px;\r\n    }\r\n\r\n    .education-content {\r\n        height: 110px;\r\n        font-size: 14px;\r\n        padding: 13px 10px 10px 15px;\r\n    }\r\n\r\n    .education-content p {\r\n        font-size: 14px;\r\n    }\r\n\r\n}\r\n\r\n/* Ipad Mini */\r\n@media only screen and (min-width: 760px) {\r\n\r\n    .container-two h4 {\r\n        font-size: larger;\r\n    }\r\n\r\n    .about-me p {\r\n        font-size: 18px;\r\n    }\r\n\r\n    .about-links {\r\n        font-size: large;\r\n    }\r\n\r\n    /* Skills Tab */\r\n    .skills-content {\r\n        height: 150px;\r\n        width: 150px;\r\n    }\r\n\r\n    .icon-class {\r\n        color: #395144;\r\n        font-size: 48px;\r\n        padding: 30px 0 0 0;\r\n    }\r\n\r\n    .skills-content p {\r\n        margin: 15px 0;\r\n        font-size: 16px;\r\n    }\r\n\r\n    /* Experiences Tab */\r\n    .experiences-content {\r\n        height: 110px;\r\n        font-size: 14px;\r\n        padding: 13px 0 5px 15px;\r\n    }\r\n\r\n    .experiences-content p {\r\n        margin: 5px 0;\r\n        font-size: 14px;\r\n    }\r\n\r\n    .education-content {\r\n        height: 110px;\r\n        font-size: 14px;\r\n        padding: 13px 10px 10px 15px;\r\n    }\r\n\r\n    .education-content p {\r\n        font-size: 14px;\r\n    }\r\n\r\n}\r\n\r\n/* Nest Hub */\r\n@media only screen and (min-width: 1000px) {\r\n\r\n    .container-two {\r\n        display: flex;\r\n    }\r\n   